Proceso sin_titulo
	Definir A1,A2,OPCION COMO REAL;
	Escribir "SELECCIONES OPCCION";
	Escribir "1:CELSUIS   2: FAREHEIT";
	Leer OPCION;
	
	
	SI OPCION==1 Entonces
		Escribir "INGRESE grados kelvin ";
		Leer A2;
		A1<-A2-273.15;
		Escribir "kelvin a celsuis " ,  A1 ;
	SiNo
		SI OPCION ==2 Entonces
			Escribir "INGRESA CENTIGRADOS";
			Leer A2;
			A1<-A2*9/5-459.67;
			Escribir " CELSISUS A FAHREHEIR ES DE " ,  A1 ;
		FinSi
	FinSi
FinProceso
